{
    "name": "dainsys/support-tickets",
    "description": "Support and recipients management package for Dainsys. This package allows you to Associate recipients to your department files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yismen.jorge@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^9.21|^10.0|^11.0",
        "intervention/image": "^2.7",
        "laravel/ui": "^4.0",
        "livewire/livewire": "^3.0",
        "owen-it/laravel-auditing": "^13.0",
        "rappasoft/laravel-livewire-tables": "^3.0",
        "php-flasher/flasher-sweetalert-laravel": "^1.1",
        "maatwebsite/excel": "^3.1",
        "asantibanez/livewire-charts": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "7.x-dev",
        "nunomaduro/collision": "v6.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Dainsys\\Support\\Database\\Factories\\": "database/factories/",
            "Dainsys\\Support\\Database\\Seeders\\": "database/seeders/",
            "Dainsys\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dainsys\\Support\\Tests\\": "tests/",
            "App\\Models\\": "tests/Models/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\Support\\SupportServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}